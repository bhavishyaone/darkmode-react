{
  "name": "darkmode-react-component",
  "private": false,
  "version": "0.1.7",
  "author": "lucasferreiralimax (https://github.com/lucasferreiralimax)",
  "bugs": {
    "url": "https://github.com/livresaber/darkmode-react/issues"
  },
  "repository": "https://github.com/livresaber/darkmode-react",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/darkmode-react-component.umd.cjs",
  "module": "./dist/darkmode-react-component.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/darkmode-react-component.js",
      "require": "./dist/darkmode-react-component.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc -b && vite build",
    "build:module": "tsc && vite build --config vite-module.config.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "e2e": "start-server-and-test 'npm run dev' :3000 'cypress open --e2e'"
  },
  "dependencies": {
    "darkmode-react-component": "^0.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "cypress": "^13.13.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.0",
    "start-server-and-test": "^2.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-style-inject": "^0.0.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  }
}
